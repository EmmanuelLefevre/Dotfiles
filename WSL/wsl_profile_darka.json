{
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "os",
          "style": "diamond",
          "background": "#81007F",
          "foreground": "#ffffff",
          "leading_diamond": "ÓÇ≤",
          "properties": {
            "alpine": "ÔåÄ",
            "arch": "ÔåÉ",
            "centos": "ÔåÑ",
            "debian": "ÔåÜ",
            "elementary": "Ôåâ",
            "fedora": "Ôåä",
            "gentoo": "Ôåç",
            "linux": "Óúí",
            "macos": "Óúë",
            "manjaro": "Ôåí",
            "mint": "Ôåè",
            "opensuse": "Ôåî",
            "raspbian": "Ôåï",
            "ubuntu": "Ôåú",
            "windows": "Óúè"
          },
          "template": " {{ .Icon }} "
        },
        {
          "type": "shell",
          "style": "powerline",
          "background": "#ffffff",
          "foreground": "#81007F",
          "powerline_symbol": "ÓÇ∞",
          "template": " Ôíâ {{ .Name }}"
        },
        {
          "type": "session",
          "style": "powerline",
          "background": "#81007F",
          "foreground": "#ffffff",
          "powerline_symbol": "ÓÇ∞",
          "template": " {{ .UserName }} "
        },
        {
          "type": "executiontime",
          "style": "powerline",
          "background": "#ffffff",
          "foreground": "#81007F",
          "powerline_symbol": "ÓÇ∞",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "template": " \ueba2 {{ .FormattedMs }}\u2800"
        },
        {
          "type": "git",
          "style": "powerline",
          "foreground": "#193549",
          "powerline_symbol": "ÓÇ∞",
          "background_templates": [
            "{{ if or (eq .Ref \"master\") (eq .Ref \"main\") }}#b431de{{ end }}",
            "{{ if eq .Ref \"dev\" }}#17d7d7{{ end }}",
            "{{ if contains .Ref \"feature\" }}#17D7A0{{ end }}",
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FB{{ end }}"
          ],
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{ if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} ÔÅÑ {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }} ÔÅÜ {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }} ÔÉá {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "Óú• ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true,
            "untracked_modes": {
              "/Users/user/Projects/oh-my-posh/": "no"
            },
            "source": "cli",
            "mapped_branches": {
              "feat/*": "üöÄ ",
              "bug/*": "üêõ "
            }
          }
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "battery",
          "style": "diamond",
          "foreground": "#81007F",
          "background": "#ffffff",
          "leading_diamond": "ÓÇ≤",
          "trailing_diamond": "ÓÇ∞",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "charged_icon": "ÓàØ  ",
            "charging_icon": "Û±ä• ",
            "discharging_icon": "Óà±  "
          },
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if and (eq \"Charged\" .State.String) (ge .Percentage 90)}}#00f826{{end}}",
            "{{if and (eq \"Charged\" .State.String) (lt .Percentage 90) (ge .Percentage 20)}}#ffffff{{end}}",
            "{{if and (eq \"Discharging\" .State.String) (lt .Percentage 20) (ge .Percentage 10)}}#ff9003{{end}}",
            "{{if and (eq \"Discharging\" .State.String) (lt .Percentage 10)}}#ff4545{{end}}"
          ],
          "foreground_templates": [
            "{{if and (eq \"Discharging\" .State.String) (lt .Percentage 20)}}#ffffff{{end}}"
          ],
          "template": " {{ if not .Error }}{{ if eq .State.String \"Charging\" }}Û±ä• {{ else if eq .State.String \"Full\" }}ÓàØ {{ else if lt .Percentage 20 }}Óà± {{ else }}Û∞Ñå {{ end }}{{ .Percentage }}{{ end }}{{ .Error }}Ôäï "
        },
        {
          "type": "sysinfo",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#ffffff",
          "background": "#81007F",
          "template": " Óâ¶  {{ round .PhysicalPercentUsed .Precision }}% ({{ (div ((sub .PhysicalTotalMemory .PhysicalFreeMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB)"
        },
        {
          "type": "docker",
          "style": "powerline",
          "powerline_symbol": "ÓÇ∞",
          "foreground": "#011627",
          "background": "#0B59E7",
          "template": " Ôåà {{ .Context }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "‚ï≠‚îÄ"
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "#81007F",
          "properties": {
            "time_format": "15:04"
          },
          "template": " ÔÖº  {{ .CurrentDate | date .Format }} |"
        },
        {
          "type": "path",
          "style": "diamond",
          "trailing_diamond": "<#ffffff></>",
          "properties": {
            "style": "agnoster_short",
            "max_depth": 5,
            "home_icon": "Ó¨Ü ",
            "folder_separator_icon": "<#81007F>Ôë† </>",
            "mapped_locations_enabled": true,
            "mapped_locations": {
              "/mnt/c/Users/Darka": "Darka"
            }
          },
          "template": " {{ .Path }}"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#81007F",
          "template": "‚ï∞\u2500"
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "#81007F",
          "foreground_templates": ["{{ if gt .Code 0 }}#fc0c07{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "template": "{{ if gt .Code 0 }} Óà∫ {{ end }}\ue285\ueab6 "
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FEF5ED",
    "template": "ÓäÖ "
  },
  "version": 2
}