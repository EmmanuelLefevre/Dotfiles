{
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "root",
          "foreground": "#ff0000",
          "background": "#ffffff",
          "style": "diamond",
          "leading_diamond": "",
          "template": "  ",
          "trailing_diamond": "<transparent,#FEF5ED></>"
        },
        {
          "background": "#028a87",
          "foreground": "#011627",
          "leading_diamond": "",
          "properties": {
            "macos": " ",
            "ubuntu": " ",
            "windows": " "
          },
          "style": "diamond",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "trailing_diamond": "<transparent,#028a87></>",
          "type": "os"
        },
        {
          "background": "#FEF5ED",
          "foreground": "#028a87",
          "leading_diamond": "",
          "style": "diamond",
          "template": " {{ .Name }}",
          "trailing_diamond": "<transparent,#FEF5ED></>",
          "type": "shell"
        },
        {
          "background": "#028a87",
          "foreground": "#011627",
          "leading_diamond": "",
          "style": "diamond",
          "template": " MEM: {{ round .PhysicalPercentUsed .Precision }}% | {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB  ",
          "trailing_diamond": "<transparent,#028a87></>",
          "type": "sysinfo"
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#d6deeb",
          "background": "#575656",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "template": " \ueba2 {{ .FormattedMs }}\u2800"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "git",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "<transparent,#17D7A0></>",
          "foreground": "#011627",
          "background": "#17D7A0",
          "properties": {
            "branch_icon": " ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} "
        },
        {
          "type": "node",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "<transparent,#303030></>",
          "foreground": "#3C873A",
          "background": "#303030",
          "template": " \ued44  {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }} "
        },
        {
          "type": "angular",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "<transparent,#ffffff></>",
          "foreground": "#ce092f",
          "background": "#ffffff",
          "template": " \ue753 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "php",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "<transparent,#09f8f4></>",
          "foreground": "#011627",
          "background": "#09f8f4",
          "template": " \ue608 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "java",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "<transparent,#0e8ac8></>",
          "foreground": "#ffffff",
          "background": "#0e8ac8",
          "template": "\ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}"
        },
        {
          "type": "dotnet",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "<transparent,#0e0e0e></>",
          "foreground": "#0d6da8",
          "background": "#0e0e0e",
          "template": "\ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }}"
        },
        {
          "type": "battery",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#011627",
          "background": "#028a87",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "charged_icon": "\uf240 ",
            "charging_icon": "\uf1e6 ",
            "discharging_icon": "\ue234 "
          },
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}{{ .Error }}\uf295 "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#ffffff",
          "style": "plain",
          "template": "╭─",
          "type": "text"
        },
        {
          "properties": {
            "time_format": "15:04"
          },
          "style": "plain",
          "template": " ♥ {{ .CurrentDate | date .Format }} |",
          "type": "time"
        },
        {
          "properties": {
            "folder_icon": " ",
            "folder_separator_icon": "  ",
            "home_icon": " "
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#028a87",
          "style": "plain",
          "template": "\u2570\u2500",
          "type": "text"
        },
        {
          "foreground": "#028a87",
          "foreground_templates": ["{{ if gt .Code 0 }}#ef5350{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "style": "plain",
          "template": "\ue285\ueab6 ",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FEF5ED",
    "template": " "
  },
  "version": 2
}