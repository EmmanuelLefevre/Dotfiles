{
  "blocks": [
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "root",
          "style": "diamond",
          "foreground": "#ff0000",
          "background": "#ffffff",
          "leading_diamond": "",
          "template": "  ",
          "trailing_diamond": "<transparent,#ffffff></>"
        },
        {
          "type": "os",
          "style": "diamond",
          "background": "#028a87",
          "foreground": "#011627",
          "leading_diamond": "",
          "trailing_diamond": "<transparent,#028a87></>",
          "properties": {
            "macos": " ",
            "ubuntu": " ",
            "windows": " "
          },
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}}"
        },
        {
          "type": "shell",
          "style": "diamond",
          "background": "#ffffff",
          "foreground": "#028a87",
          "leading_diamond": "",
          "template": " {{ .Name }}",
          "trailing_diamond": "<transparent,#ffffff></>"
        },
        {
          "type": "sysinfo",
          "style": "diamond",
          "background": "#028a87",
          "foreground": "#011627",
          "leading_diamond": "",
          "trailing_diamond": "<transparent,#028a87></>",
          "template": " MEM: {{ round .PhysicalPercentUsed .Precision }}% | {{ (div ((sub .PhysicalTotalMemory .PhysicalAvailableMemory)|float64) 1073741824.0) }}/{{ (div .PhysicalTotalMemory 1073741824.0) }}GB  "
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "leading_diamond": "",
          "trailing_diamond": "",
          "foreground": "#d6deeb",
          "background": "#575656",
          "properties": {
            "style": "roundrock",
            "threshold": 0
          },
          "template": " \ueba2 {{ .FormattedMs }}\u2800"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "type": "node",
          "style": "powerline",
          "foreground": "#3C873A",
          "properties": {
            "fetch_version": true
          },
          "template": " \ued44  {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}"
        },
        {
          "type": "angular",
          "style": "powerline",
          "foreground": "#ce092f",
          "properties": {
            "fetch_version": true
          },
          "template": "<#028a87> \u007C </>\ue753</> {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "php",
          "style": "powerline",
          "foreground": "#011627",
          "properties": {
            "fetch_version": true
          },
          "template": " \ue608 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "dotnet",
          "style": "powerline",
          "foreground": "#0d6da8",
          "properties": {
            "fetch_version": true
          },
          "template": " \ue77f {{ if .Unsupported }}\uf071{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "java",
          "style": "powerline",
          "foreground": "#ffffff",
          "properties": {
            "fetch_version": true
          },
          "template": " \ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} "
        },
        {
          "type": "git",
          "style": "diamond",
          "foreground": "#011627",
          "background": "#17D7A0",
          "leading_diamond": "",
          "trailing_diamond": "<transparent,#17D7A0></>",
          "properties": {
            "branch_icon": " ",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": " {{ .UpstreamIcon }}{{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }}  {{ .Working.String }}{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Staging.Changed }}  {{ .Staging.String }}{{ end }}{{ if gt .StashCount 0 }}  {{ .StashCount }}{{ end }} "
        },
        {
          "type": "battery",
          "style": "diamond",
          "foreground": "#011627",
          "background": "#028a87",
          "leading_diamond": "",
          "trailing_diamond": "",
          "invert_powerline": true,
          "powerline_symbol": "\ue0b2",
          "properties": {
            "charged_icon": "  ",
            "charging_icon": "󱊥 ",
            "discharging_icon": "  "
          },
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if and (eq \"Charged\" .State.String) (ge .Percentage 90)}}#00f826{{end}}",
            "{{if and (eq \"Charged\" .State.String) (lt .Percentage 90) (ge .Percentage 20)}}#028a87{{end}}",
            "{{if and (eq \"Discharging\" .State.String) (lt .Percentage 20) (ge .Percentage 10)}}#ff9003{{end}}",
            "{{if and (eq \"Discharging\" .State.String) (lt .Percentage 10)}}#ff4545{{end}}"
          ],
          "foreground_templates": [
            "{{if and (eq \"Discharging\" .State.String) (lt .Percentage 20)}}#ffffff{{end}}"
          ],
          "template": " {{ if not .Error }}{{ if eq .State.String \"Charging\" }}󱊥 {{ else if eq .State.String \"Full\" }} {{ else if lt .Percentage 20 }} {{ else }}󰄌 {{ end }}{{ .Percentage }}{{ end }}{{ .Error }} "
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#ffffff",
          "template": "╭─"
        },
        {
          "type": "time",
          "style": "plain",
          "properties": {
            "time_format": "15:04"
          },
          "template": " 󰂧 {{ .CurrentDate | date .Format }} |"
        },
        {
          "type": "path",
          "style": "diamond",
          "trailing_diamond": "<#ffffff></>",
          "properties": {
            "style": "agnoster_full",
            "home_icon": " ",
            "folder_separator_icon": " <#028a87></><#ffffff>  </>"
          },
          "template": " {{ .Path }}"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "#028a87",
          "template": "╰\u2500"
        },
        {
          "type": "status",
          "style": "plain",
          "foreground": "#028a87",
          "foreground_templates": ["{{ if gt .Code 0 }}#ef5350{{ end }}"],
          "properties": {
            "always_enabled": true
          },
          "template": "{{ if gt .Code 0 }}  {{ end }}\ue285\ueab6 "
        }
      ],
      "type": "prompt"
    }
  ],
  "console_title_template": "{{ .Folder }}",
  "transient_prompt": {
    "background": "transparent",
    "foreground": "#FEF5ED",
    "template": " "
  },
  "version": 2
}